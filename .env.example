# Momentum Environment Variables
# Copy this file to .env and fill in your values

# RevenueCat API Keys
# Get these from RevenueCat Dashboard > Project Settings > API Keys
EXPO_PUBLIC_REVENUECAT_ANDROID_KEY=goog_your_android_key_here
EXPO_PUBLIC_REVENUECAT_IOS_KEY=appl_your_ios_key_here

# Feature Flags
EXPO_PUBLIC_ENABLE_DEBUG_SCREEN=true

# Gemini API Key
# Get yours free at https://aistudio.google.com/apikey
EXPO_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here
# Gemini model (default: gemini-2.0-flash)
EXPO_PUBLIC_GEMINI_MODEL=gemini-2.0-flash

# Optional: Analytics (future)
# EXPO_PUBLIC_ANALYTICS_KEY=your_analytics_key

# Optional: Sentry (future)
# EXPO_PUBLIC_SENTRY_DSN=your_sentry_dsn
