{
  "cli": {
    "version": ">= 5.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_REVENUECAT_ANDROID_KEY": "YOUR_DEV_KEY",
        "EXPO_PUBLIC_REVENUECAT_IOS_KEY": "YOUR_DEV_KEY",
        "EXPO_PUBLIC_GEMINI_API_KEY": "YOUR_GEMINI_KEY",
        "EXPO_PUBLIC_GEMINI_MODEL": "gemini-2.0-flash"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_REVENUECAT_ANDROID_KEY": "YOUR_PREVIEW_KEY",
        "EXPO_PUBLIC_REVENUECAT_IOS_KEY": "YOUR_PREVIEW_KEY",
        "EXPO_PUBLIC_GEMINI_API_KEY": "YOUR_GEMINI_KEY",
        "EXPO_PUBLIC_GEMINI_MODEL": "gemini-2.0-flash"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_REVENUECAT_ANDROID_KEY": "YOUR_PROD_KEY",
        "EXPO_PUBLIC_REVENUECAT_IOS_KEY": "YOUR_PROD_KEY",
        "EXPO_PUBLIC_GEMINI_API_KEY": "YOUR_GEMINI_KEY",
        "EXPO_PUBLIC_GEMINI_MODEL": "gemini-2.0-flash"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}
